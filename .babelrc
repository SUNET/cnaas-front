{
  "presets": [
    [
      "@babel/preset-env",
      { "targets": { "node": "current" }, "shippedProposals": true }
    ],
    ["@babel/preset-react", { "runtime": "automatic" }]
  ],
  "plugins": [
    [
      "prismjs",
      {
        "languages": ["javascript", "css", "diff"],
        "plugins": ["diff-highlight"],
        "theme": "default",
        "css": false
      }
    ]
  ]
}
